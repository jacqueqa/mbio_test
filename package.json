{
  "name": "mbio_test",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/jacqueqa/js_mbio",
  "author": "Jacqueline Pereira",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "allure-commandline": "^2.17.2",
    "allure-playwright": "^2.9.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-plugin-playwright": "^0.19.0",
    "playwright": "^1.40.0"
  },
  "scripts": {
    "test:e2e": "yarn playwright test --project e2e --config playwright.config.js",
    "test:e2e:ci": "yarn test:e2e --workers 2",
    "allure:generate": "yarn allure generate ./allure-results --clean -o allure-report",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true",
    "allure:open": "npx playwright show-report allure-report"
  },
  "keywords": [
    "Plawright",
    "Test Automation",
    "JavaScript"
  ]
}
